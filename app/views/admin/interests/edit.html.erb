<% @title = '関心事編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @interest_form, as: 'form',
               url: [ :admin, @interest_form.interest ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= link_to 'キャンセル', :admin_interests %>
    </div>
  <% end %>
</div>
