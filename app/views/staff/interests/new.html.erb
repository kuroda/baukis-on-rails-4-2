<% @title = 'New Interest' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @interest_form, as: 'form', url: :staff_interests do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= link_to 'キャンセル', :staff_interests %>
    </div>
  <% end %>
</div>